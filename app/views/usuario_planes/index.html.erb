<p id="notice"><%= notice %></p>

<h1>Usuario Planes</h1>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Plan</th>
      <th>Nro personas</th>
      <th>Fecha asignacion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuario_planes.each do |usuario_plan| %>
      <tr>
        <td><%= usuario_plan.Usuario %></td>
        <td><%= usuario_plan.Plan %></td>
        <td><%= usuario_plan.nro_personas %></td>
        <td><%= usuario_plan.fecha_asignacion %></td>
        <td><%= usuario_plan.estatus %></td>
        <td><%= link_to 'Show', usuario_plan %></td>
        <td><%= link_to 'Edit', edit_usuario_plan_path(usuario_plan) %></td>
        <td><%= link_to 'Destroy', usuario_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario Plan', new_usuario_plan_path %>
