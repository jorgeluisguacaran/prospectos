<p id="notice"><%= notice %></p>

<h1>Tipo Correos</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_correos.each do |tipo_correo| %>
      <tr>
        <td><%= tipo_correo.descripcion %></td>
        <td><%= tipo_correo.estatus %></td>
        <td><%= link_to 'Show', tipo_correo %></td>
        <td><%= link_to 'Edit', edit_tipo_correo_path(tipo_correo) %></td>
        <td><%= link_to 'Destroy', tipo_correo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Correo', new_tipo_correo_path %>
