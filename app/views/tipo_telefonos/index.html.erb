<p id="notice"><%= notice %></p>

<h1>Tipo Telefonos</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_telefonos.each do |tipo_telefono| %>
      <tr>
        <td><%= tipo_telefono.descripcion %></td>
        <td><%= tipo_telefono.estatus %></td>
        <td><%= link_to 'Show', tipo_telefono %></td>
        <td><%= link_to 'Edit', edit_tipo_telefono_path(tipo_telefono) %></td>
        <td><%= link_to 'Destroy', tipo_telefono, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Telefono', new_tipo_telefono_path %>
